# PostgreSQL Configuration
POSTGRES_USER=admin@admin.com
POSTGRES_PASSWORD=admin

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@admin.com
PGADMIN_DEFAULT_PASSWORD=admin

# MySQL Configuration
MYSQL_ROOT_PASSWORD=rootpass
MYSQL_DATABASE=devdb
MYSQL_USER=user
MYSQL_PASSWORD=userpass

# Grafana Cloud - Prometheus Configuration
# Get these from: https://grafana.com/docs/grafana-cloud/account-management/authentication-and-permissions/access-policies/
GCLOUD_PROMETHEUS_URL=https://prometheus-<region>.grafana.net/api/prom/push
GCLOUD_PROMETHEUS_USERNAME=your_prometheus_username
GCLOUD_PROMETHEUS_PASSWORD=your_prometheus_api_key

# Grafana Cloud - Loki Configuration
# Get these from: https://grafana.com/docs/grafana-cloud/account-management/authentication-and-permissions/access-policies/
GCLOUD_LOKI_URL=https://logs-prod-<region>.grafana.net/loki/api/v1/push
GCLOUD_LOKI_USERNAME=your_loki_username
GCLOUD_LOKI_PASSWORD=your_loki_api_key
